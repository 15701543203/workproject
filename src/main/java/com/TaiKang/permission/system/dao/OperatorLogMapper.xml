<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.TaiKang.permission.system.dao.OperatorLogMapper">
    <insert id="insertOperatorLog" parameterType="com.TaiKang.permission.system.bean.OperatorLog" useGeneratedKeys="true" keyProperty="per_id">
        INSERT INTO operator_log (
            user_id,
            user_name,
            true_name,
            role_name,
            oper_date,
            oper_type,
            oper_content
        )
        VALUES
            (
            #{userId},
            #{username},
            #{trueName},
            #{roleName},
            NOW(),
            #{operType},
            #{operContent}
            )
    </insert>
</mapper>
